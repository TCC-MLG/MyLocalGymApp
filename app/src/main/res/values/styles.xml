<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/common_google_signin_btn_text_dark_focused</item>
        <item name="colorPrimaryDark">@color/common_google_signin_btn_text_dark_focused</item>
        <item name="colorAccent">@color/colorlogin</item>
        //@color/colorPrimaryDark
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppMylocalteste" parent="AppTheme">
        <item name="colorPrimary">@color/common_google_signin_btn_text_dark_focused</item>
        <item name="colorAccent">@color/colorlogin</item>
        <item name="android:textColorSecondary">@color/cast_intro_overlay_background_color</item>
        <item name="android:colorForeground">
            @color/cast_libraries_material_featurehighlight_text_body_color
        </item>
        <item name="android:textColorPrimary">
            @color/cast_libraries_material_featurehighlight_text_body_color
        </item>
        <item name="android:textColorPrimaryInverse">@color/cast_intro_overlay_background_color
        </item>
        <item name="android:textColorSecondaryInverse">@color/cast_intro_overlay_background_color
        </item>
        <item name="android:windowBackground">@color/colorlogin</item>
    </style>

</resources>
